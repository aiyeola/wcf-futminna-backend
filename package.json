{
  "name": "wcf-futminna-backend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "rm -rf ./src/build && babel ./src/. --out-dir ./src/build --copy-files",
    "start": "node ./src/build/index.js",
    "lint": "pretty-quick --staged",
    "test": "nyc --reporter=html --reporter=text cross-env NODE_ENV=test mocha --require @babel/register ./src/__test__/*.js --timeout 60000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "argon2": "^0.27.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.2",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.12.9",
    "morgan": "^1.10.0",
    "redis": "^3.1.2",
    "swagger-node-express": "^2.1.3",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0"
  }
}
